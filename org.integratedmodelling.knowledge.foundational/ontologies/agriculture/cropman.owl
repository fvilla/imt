<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
	xmlns:owl="http://www.w3.org/2002/07/owl#"
	xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
	xmlns:persistence="http://www.integratedmodelling.org/ks/thinklab/persistence.owl#"
	xmlns="http://ontologies.seamless-ip.org/cropman.owl#" 
	xmlns:activity="http://ontologies.seamless-ip.org/activity.owl#"
	xmlns:agrirule="http://ontologies.seamless-ip.org/agrirule.owl#"
	xmlns:crop="http://ontologies.seamless-ip.org/crop.owl#"
	xmlns:farm="http://ontologies.seamless-ip.org/farm.owl#"
	xmlns:prodent="http://ontologies.seamless-ip.org/prodent.owl#"
	xml:base="http://ontologies.seamless-ip.org/cropman.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://ontologies.seamless-ip.org/activity.owl"/>
    <owl:imports rdf:resource="http://ontologies.seamless-ip.org/agrirule.owl"/>
    <owl:imports rdf:resource="http://ontologies.seamless-ip.org/crop.owl"/>
    <owl:imports rdf:resource="http://ontologies.seamless-ip.org/prodent.owl"/>
    <owl:imports rdf:resource="http://ontologies.seamless-ip.org/farm.owl"/>
    <rdfs:comment xml:lang="en">The crop management ontology specifies both simple management activities (i.e. through the SEAMLESS ZALF surveys) and more complex management activities, i.e. those generated by the Production Technique Generator (PTG) of SEAMLESS.</rdfs:comment>
  </owl:Ontology>
  
  	<rdf:Description rdf:about="http://ontologies.seamless-ip.org/crop.owl#hasProduct">
		<rdfs:domain>
			<owl:Class>
				<owl:unionOf rdf:parseType="Collection">
					<rdf:Description rdf:about="#SimpleCropManagementinZone" />
					<rdf:Description rdf:about="#ProductOnSoil" />
					<rdf:Description rdf:about="#SimpleSurveyRotationElement" />
				</owl:unionOf>
			</owl:Class>
		</rdfs:domain>
	</rdf:Description>
    
  
  
  <owl:Class rdf:ID="ManagementProcedureAndTiming">
    
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:ObjectProperty rdf:ID="hasTimeDecade"/>
        </owl:onProperty>
        <owl:maxCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int">2</owl:maxCardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CropManagementInZones">
    
    <persistence:factory rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</persistence:factory>
  </owl:Class>
  <owl:Class rdf:ID="SimpleCurrentBeefActivity">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SimpleLivestockActivity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SimpleCurrentDairyActivity">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SimpleLivestockActivity"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="RegionalAgroManagementZone">
    
  </owl:Class>
  <owl:Class rdf:about="#SimpleLivestockActivity">
    <persistence:factory rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</persistence:factory>
    
  </owl:Class>
  <owl:Class rdf:ID="SurveyActivities">
    <persistence:factory rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</persistence:factory>
    
  </owl:Class>
  <owl:Class rdf:ID="CostandLabourPerRegionalZone">
    
  </owl:Class>
  <owl:Class rdf:ID="SimpleSurveyRotationElement">
    <rdfs:subClassOf rdf:resource="#SurveyActivities"/>
  </owl:Class>
  <owl:Class rdf:ID="ManagementProcedure">
    
    
  </owl:Class>
  <owl:Class rdf:ID="SurveyCropRotationElement">
    
    <rdfs:subClassOf rdf:resource="#SurveyActivities"/>
  </owl:Class>
  <owl:Class rdf:ID="InputOnSoilInManagementProcedure">
    <rdfs:subClassOf rdf:resource="#CropManagementInZones"/>
  </owl:Class>
  <owl:Class rdf:ID="ProductOnSoil">
    
  </owl:Class>
  <owl:Class rdf:ID="SimpleCurrentSmallDairyRuminantActivity">
    <rdfs:subClassOf rdf:resource="#SimpleLivestockActivity"/>
  </owl:Class>
  <owl:Class rdf:ID="SimpleCurrentSmallBeefRuminants">
    <rdfs:subClassOf rdf:resource="#SimpleLivestockActivity"/>
  </owl:Class>
  <owl:Class rdf:ID="SimpleCropManagementinZone">
    <rdfs:subClassOf rdf:resource="#CropManagementInZones"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="hasYear">
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/prodent.owl#RotationYear"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#SurveyActivities"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasRegionalAgroManagementZone">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#ProductOnSoil"/>
          <owl:Class rdf:about="#CropManagementInZones"/>
          <owl:Class rdf:about="#CostandLabourPerRegionalZone"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="#RegionalAgroManagementZone"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInputsAndImplements">
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/agrirule.owl#InputsAndImplements"/>
    <rdfs:domain rdf:resource="#ManagementProcedure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasManagementProcedureWithTiming">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#InputOnSoilInManagementProcedure"/>
    <rdfs:range rdf:resource="#ManagementProcedureAndTiming"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasAgriEnvironmentalZones">
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/farm.owl#AgriEnvironmentalZone"/>
    <rdfs:domain rdf:resource="#RegionalAgroManagementZone"/>
    
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSimpleCropManagement">
    <rdfs:domain rdf:resource="#SimpleCropManagementinZone"/>
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/activity.owl#SimpleCropManagement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasManagementProcedure">
    <rdfs:domain rdf:resource="#ManagementProcedureAndTiming"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="#ManagementProcedure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInput">
    <rdfs:domain rdf:resource="#InputOnSoilInManagementProcedure"/>
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/agrirule.owl#Input"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasTimeDecade">
    <rdfs:domain rdf:resource="#ManagementProcedureAndTiming"/>
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/agrirule.owl#TimeDecade"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSurveyActivities">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:domain rdf:resource="http://ontologies.seamless-ip.org/farm.owl#NUTSregion"/>
    <rdfs:range rdf:resource="#SurveyActivities"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasNUTSRegion"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasLivestockActivities">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
    <rdfs:range rdf:resource="#SimpleLivestockActivity"/>
    <rdfs:domain rdf:resource="http://ontologies.seamless-ip.org/farm.owl#NUTSregion"/>
    <owl:inverseOf>
      <owl:ObjectProperty rdf:ID="hasRegion"/>
    </owl:inverseOf>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasInputOnSoilInManagementProcedure">
    <rdfs:domain rdf:resource="#SurveyCropRotationElement"/>
    <rdfs:range rdf:resource="#InputOnSoilInManagementProcedure"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCrop">
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/crop.owl#Crop"/>
    <rdfs:domain rdf:resource="#SurveyCropRotationElement"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasNUTSRegion">
    <owl:inverseOf rdf:resource="#hasSurveyActivities"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/farm.owl#NUTSregion"/>
    <rdfs:domain rdf:resource="#SurveyActivities"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasCostsandLabour">
    <rdfs:domain rdf:resource="#SurveyCropRotationElement"/>
    <rdfs:range rdf:resource="#CostandLabourPerRegionalZone"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasSimpleCropManagementinZone">
    <rdfs:range rdf:resource="#SimpleCropManagementinZone"/>
    <rdfs:domain rdf:resource="#SimpleSurveyRotationElement"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="hasProductOnSoil">
    <rdfs:domain rdf:resource="#SurveyCropRotationElement"/>
    <rdfs:range rdf:resource="#ProductOnSoil"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:about="#hasRegion">
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <owl:inverseOf rdf:resource="#hasLivestockActivities"/>
    <rdfs:range rdf:resource="http://ontologies.seamless-ip.org/farm.owl#NUTSregion"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="costsOfVeterinary">
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceOfAdultFemaleAtSelling">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallBeefRuminants"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceForLambOrGoatling">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:domain rdf:resource="#SimpleCurrentSmallBeefRuminants"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="costsOfReplacement">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="rotationLength">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SurveyActivities"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="costsOfInsemination">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="shapeFileID">
    <rdfs:domain rdf:resource="#RegionalAgroManagementZone"/>
    <rdfs:subPropertyOf rdf:resource="http://www.integratedmodelling.org/ks/thinklab/persistence.owl#text"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="lengthOfFatteningPeriod">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentBeefActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallBeefRuminants"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="numbeofbirthsperadultfemale">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="grossMargin">
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="yield">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#ProductOnSoil"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="amountRequired">
    
    <rdfs:domain rdf:resource="#InputOnSoilInManagementProcedure"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="weightAtEndOfFattening">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentBeefActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallBeefRuminants"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceForCalve">
    
    <rdfs:domain rdf:resource="#SimpleCurrentBeefActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="milkProduction">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="replacementRate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ageAtFirstBirth">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="weightOfCarcass">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentBeefActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallBeefRuminants"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceOfCattleAtSelling">
    
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SimpleCurrentBeefActivity"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="revenuesFromMilk">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="lossRate">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="amountOfConcentrates">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="isGoat">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallBeefRuminants"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="totalRevenues">
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="soldMilk">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#SimpleCurrentDairyActivity"/>
          <owl:Class rdf:about="#SimpleCurrentSmallDairyRuminantActivity"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="weightOfCalveAtSelling">
    <rdfs:domain rdf:resource="#SimpleCurrentDairyActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="costsOfConcentrates">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SimpleLivestockActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="gainPerSheepOrGoat">
    
    <rdfs:domain rdf:resource="#SimpleCurrentSmallBeefRuminants"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ageOfSheepOrGoatAtSelling">
    
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SimpleCurrentSmallBeefRuminants"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="priceForFemaleCalvesAtSelling">
    
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SimpleCurrentDairyActivity"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="ageOfAdultFemaleAtSelling">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
    <rdfs:domain>
      <owl:Class>

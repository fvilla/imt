<?xml version="1.0"?>
<rdf:RDF
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns:xsp="http://www.owl-ontologies.com/2005/08/07/xsp.owl#"
    xmlns:prodent="http://ontologies.seamless-ip.org/prodent.owl#"
    xmlns:dc="http://purl.org/dc/elements/1.1/"
    xmlns:indi="http://ontologies.seamless-ip.org/indi.owl#"
    xmlns:activity="http://ontologies.seamless-ip.org/activity.owl#"
    xmlns:swrlb="http://www.w3.org/2003/11/swrlb#"
    xmlns:livestock="http://ontologies.seamless-ip.org/livestock.owl#"
    xmlns:crop="http://ontologies.seamless-ip.org/crop.owl#"
    xmlns:farmopt="http://ontologies.seamless-ip.org/farmopt.owl#"
    xmlns:cropman="http://ontologies.seamless-ip.org/cropman.owl#"
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns="http://ontologies.seamless-ip.org/farm.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:seamproj="http://ontologies.seamless-ip.org/seamproj.owl#"
    xmlns:swrl="http://www.w3.org/2003/11/swrl#"
    xmlns:capri="http://ontologies.seamless-ip.org/capri.owl#"
    xmlns:agrirule="http://ontologies.seamless-ip.org/agrirule.owl#"
    xmlns:persistence="http://www.integratedmodelling.org/ks/thinklab/persistence.owl#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xml:base="http://ontologies.seamless-ip.org/farm.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl"/>
    <owl:imports rdf:resource="http://www.integratedmodelling.org/ks/thinklab/persistence.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="RepresentativeFarm">
    <rdfs:comment xml:lang="en">Representative farm holds the data on a collection of farms found in FADN regions for farm types with a specialisation, intensity-class and size-class, for example an arable-specialised crops farm in the Netherlands with medium intensity and large size-class</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RepresentativeFarmGroup">
    <rdfs:comment xml:lang="en">Farm type groups of representative farms. Used for structural change analyses. Do not include the intensity dimension of the SEAMLESS farm typology</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="EnvironmentalZone">
    <rdfs:comment xml:lang="en">Environmental zones are a division of Europe into 13 zones based primarily on climate, altitude, oceanicity and northing (see Metzger et al., 2005). In SEAMLESS the environmental zones are overlayed with NUTS regions to for climate zones (Hazeu et al., 2009)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="DailyClimate">
    <rdfs:comment xml:lang="en">A set of climate variables at a daily level for the SEAMLESS climate zones. The variables are calculated from the JRC/MARS  50 km grid data</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="LivestockInformationAnnual">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="LivestockInformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="FarmSize">
    <rdfs:comment xml:lang="en">Size dimension of the SEAMLESS farm typology based on so-called European Size Units (ESU)</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:ID="Region"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="FarmSpecialization"/>
    </owl:disjointWith>
    <owl:disjointWith>
      <owl:Class rdf:ID="FarmIntensity"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="CropInformationTriennial">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="CropInformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:about="#Region">
    <rdfs:comment xml:lang="en">A region is a spatial unit defined by administrative or natural borders</rdfs:comment>
    <rdfs:label xml:lang="x-gams">Farm</rdfs:label>
    <persistence:factory rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</persistence:factory>
    <owl:disjointWith>
      <owl:Class rdf:about="#FarmSpecialization"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#FarmSize"/>
    <owl:disjointWith>
      <owl:Class rdf:about="#FarmIntensity"/>
    </owl:disjointWith>
  </owl:Class>
  <owl:Class rdf:ID="FADNregion">
    <rdfs:comment xml:lang="en">FADN regions are used by DG agri for the agricultural economic statistics. In most cases corresponding to NUTS 2 or NUTS 3 regions</rdfs:comment>
    <rdfs:label xml:lang="x-gams">FR</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Region"/>
  </owl:Class>
  <owl:Class rdf:about="#LivestockInformation">
    <rdfs:comment xml:lang="en">Information on livestock for the representative farms</rdfs:comment>
    <persistence:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</persistence:abstract>
  </owl:Class>
  <owl:Class rdf:ID="VolumeStones">
    <rdfs:comment xml:lang="en">Classes of volume of stones. Four classes and a no information class is included</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FarmInformationTriennial">
    <rdfs:subClassOf>
      <owl:Class rdf:ID="FarmInformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="CropInformationAnnual">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#CropInformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SoilCharacteristics">
    <rdfs:comment xml:lang="en">A set of variables describing the soil characteristics of an agri-environmental zone</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TransitionProbability">
    <rdfs:comment xml:lang="en">Probability of one representative farm within a farm type group going into transition to another farm type group</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="TexturalClassSoil">
    <rdfs:comment xml:lang="en">Soil characteristics linked to the texture class of the soils</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ClimateZone">
    <rdfs:comment xml:lang="en">Climate zones are delineated by overlaying the 13 environmental zones and the NUTS regions used in SEAMLESS. The climate zones are assumed to have homogenous climatic conditions for farming and are described by one set of daily climate variables in the SEAMLESS database</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="FarmInformationAnnual">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#FarmInformation"/>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="Soiltype">
    <rdfs:comment xml:lang="en">SEAMLESS soil type as characterised by the OCTOP classes. OCTOP stands for organic carbon content. Six types plus a no information type is distinguished</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RegionalTypology">
    <rdfs:comment xml:lang="en">Regional typologies, specifying different types of typologies that can be used as context for the assessment of indicators</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#FarmInformation">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >FarmInformation</rdfs:label>
    <persistence:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</persistence:abstract>
  </owl:Class>
  <owl:Class rdf:about="#FarmSpecialization">
    <rdfs:comment xml:lang="en">The specialisation dimension of the SEAMLESS farm typology in terms of output activity and land use</rdfs:comment>
    <owl:disjointWith>
      <owl:Class rdf:about="#FarmIntensity"/>
    </owl:disjointWith>
    <owl:disjointWith rdf:resource="#Region"/>
    <owl:disjointWith rdf:resource="#FarmSize"/>
  </owl:Class>
  <owl:Class rdf:ID="RegionalTypologyClass">
    <rdfs:comment xml:lang="en">Classes used in the different regional typologies</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="RepresentativeFarmInAgriEnvRegion">
    <rdfs:comment xml:lang="en">Allocation of representative farms to agri-environmental zones</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="Unit"/>
  <owl:Class rdf:ID="RegionalTypologyValue">
    <rdfs:comment xml:lang="en">Values for the regional typologies</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#CropInformation">
    <rdfs:comment xml:lang="en">Crop information holds all the information in FADN related to crops (area, production and outputs) in yearly values. The information is for a specific farm type in a specific region (a representative farm). Most figures are total for the farm type</rdfs:comment>
    <persistence:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean"
    >true</persistence:abstract>
  </owl:Class>
  <owl:Class rdf:ID="AgriEnvironmentalZone">
    <rdfs:comment xml:lang="en">An agri-environmental zone identifies a location within Europe as a unique combination between a soil type, an environmental zone and a NUTS region. It is thus the lowest spatial unit on which SEAMLESS builds. The agri-environmental zone is represented by one soil profile in the SEAMLESS database</rdfs:comment>
    <rdfs:label xml:lang="x-gams">S</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Region"/>
  </owl:Class>
  <owl:Class rdf:ID="NUTSregion">
    <rdfs:comment xml:lang="en">Administrative region used in SEAMLESS. NUTS system version 7 is used</rdfs:comment>
    <rdfs:label xml:lang="x-gams">R</rdfs:label>
    <rdfs:subClassOf rdf:resource="#Region"/>
  </owl:Class>
  <owl:Class rdf:ID="LivestockInformationTriennial">
    <rdfs:subClassOf rdf:resource="#LivestockInformation"/>
  </owl:Class>
  <owl:Class rdf:about="#FarmIntensity">
    <rdfs:comment xml:lang="en">Intensity dimension of the SEAMLESS farm typology in terms of output per hectare</rdfs:comment>
    <owl:disjointWith rdf:resource="#FarmSpecialization"/>
    <owl:disjointWith rdf:resource="#Region"/>
    <owl:disjointWith rdf:resource="#FarmSize"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="farmInformation">
    <rdfs:domain rdf:resource="#RepresentativeFarm"/>
    <rdfs:range rdf:resource="#FarmInformationAnnual"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="livestockInformationTriennial">
    <rdfs:range rdf:resource="#LivestockInformationTriennial"/>
    <rdfs:domain rdf:resource="#RepresentativeFarm"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="cropInformationTriennial">
    <rdfs:domain rdf:resource="#RepresentativeFarm"/>
    <rdfs:range rdf:resource="#CropInformationTriennial"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:ObjectProperty rdf:ID="farmInformationTriennial">
    <rdfs:domain rdf:resource="#RepresentativeFarm"/>
    <rdfs:range rdf:resource="#FarmInformationTriennial"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="unitDescription">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Unit"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="shareOfRFArea">
    <rdfs:domain rdf:resource="#RepresentativeFarmInAgriEnvRegion"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="unitName">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Unit"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="untiAbbreviation">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain rdf:resource="#Unit"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:DatatypeProperty rdf:ID="shareOfAEZArea">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#RepresentativeFarmInAgriEnvRegion"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="totalIntermediateConsumption">
    <dc:source>FADN:SE275</dc:source>
    <rdfs:comment xml:lang="en">Total intermediate consumption</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#FarmInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="grossInvestments">
    <dc:source>FADN:SE516</dc:source>
    <rdfs:comment xml:lang="en">Gross investments</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#FarmInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="productionFieldScaleVegetables">
    <dc:source>FADN:K136QQ</dc:source>
    <rdfs:comment xml:lang="en">Production of field-scale fresh vegetables</rdfs:comment>
    <persistence:unit>t</persistence:unit>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="averageYieldMaizePerHectare">
    <rdfs:comment xml:lang="en">Average yield of maize for the farm type</rdfs:comment>
    <persistence:unit>t/ha</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="areaSummerCerealsMixes">
    <dc:source>FADN:K125AA</dc:source>
    <rdfs:comment xml:lang="en">Area of summer cereals mixes</rdfs:comment>
    <persistence:unit>ha</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="definition">
    <rdfs:comment xml:lang="en">Definition of the item</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf rdf:parseType="Collection">
          <owl:Class rdf:about="#FarmIntensity"/>
          <owl:Class rdf:about="#FarmSize"/>
          <owl:Class rdf:about="#Soiltype"/>
          <owl:Class rdf:about="#FarmSpecialization"/>
          <owl:Class rdf:about="#TexturalClassSoil"/>
          <owl:Class rdf:about="#RegionalTypologyClass"/>
        </owl:unionOf>
      </owl:Class>
    </rdfs:domain>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="closingDairyCows">
    <dc:source>FADN:D30CN</dc:source>
    <rdfs:comment xml:lang="en">Closing value: number of dairy cows</rdfs:comment>
    <persistence:unit>head</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#LivestockInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hasNUTSRegion">
    <rdfs:comment xml:lang="en">NUTS region for which the value is given</rdfs:comment>
    <rdfs:domain rdf:resource="#RegionalTypologyValue"/>
    <rdfs:range rdf:resource="#NUTSregion"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="areaSoya">
    <dc:source>FADN:K333AA</dc:source>
    <rdfs:comment xml:lang="en">Area of soya</rdfs:comment>
    <persistence:unit>ha</persistence:unit>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="areaMushrooms">
    <dc:source>FADN:K139AA</dc:source>
    <rdfs:comment xml:lang="en">Area of mushrooms</rdfs:comment>
    <persistence:unit>ha</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="paymentsDairyOutgoers">
    <dc:source>FADN:SE407</dc:source>
    <rdfs:comment xml:lang="en">Payments to dairy outgoers</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:domain rdf:resource="#FarmInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="closingValueTableChickens">
    <dc:source>FADN:D47CV</dc:source>
    <rdfs:comment xml:lang="en">Closing value: table chickens</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:domain rdf:resource="#LivestockInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="outputNurseries">
    <dc:source>FADN:K157TP</dc:source>
    <rdfs:comment xml:lang="en">Output of nurseries</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="outputOtherSeeds">
    <dc:source>FADN:K143TP</dc:source>
    <rdfs:comment xml:lang="en">Output of other seeds</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="maximumUsableMoistureReserve">
    <rdfs:comment xml:lang="en">Maximum usable moisture reserve in the soil  profile. This a function of rooting depth (RD) and Available water holding capacity (AWC), where AWC is the difference between SMFC and SMWP (water holding capacities at field capacity and wilting point)</rdfs:comment>
    <persistence:unit>mm</persistence:unit>
    <rdfs:domain rdf:resource="#SoilCharacteristics"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="hoursHolder2">
    <dc:source>FADN:Co2HR</dc:source>
    <rdfs:comment xml:lang="en">Working hours of second holder/manager</rdfs:comment>
    <persistence:unit>h</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#FarmInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="soilCharacteristics">
    <rdfs:comment xml:lang="en">Link to the set of soil data for the agri-environmental zone</rdfs:comment>
    <rdfs:domain rdf:resource="#AgriEnvironmentalZone"/>
    <rdfs:range rdf:resource="#SoilCharacteristics"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="otherCropSpecificInputs">
    <dc:source>FADN:SE305</dc:source>
    <rdfs:comment xml:lang="en">Cost of other crop specific inputs</rdfs:comment>
    <persistence:unit>�</persistence:unit>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="milkYieldAverage">
    <rdfs:comment xml:lang="en">Average milk yield of the farm type</rdfs:comment>
    <persistence:unit>kg/cow</persistence:unit>
    <rdfs:domain rdf:resource="#LivestockInformation"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="yieldWheat">
    <dc:source>FADN:SE110</dc:source>
    <rdfs:comment xml:lang="en">Yield of wheat in quintal/ha</rdfs:comment>
    <persistence:unit>t/ha</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="closingBees">
    <dc:source>FADN:D33CN</dc:source>
    <rdfs:comment xml:lang="en">Closing value: number of bees - no. Of hives</rdfs:comment>
    <persistence:unit>head</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#LivestockInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="areaBarley">
    <dc:source>FADN:K123AA</dc:source>
    <rdfs:comment xml:lang="en">Area of barley</rdfs:comment>
    <persistence:unit>ha</persistence:unit>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#CropInformation"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
